<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>


<%= form_tag sessions_path do %>
  <div>
    <%= label_tag 'email', 'Email' %><br>
    <%= text_field_tag 'email' %>
  </div>
  <div>
    <%= label_tag 'password', 'Password' %><br>
    <%= password_field_tag 'password' %>
  </div>
  <%= submit_tag 'Login' %>
<% end %>