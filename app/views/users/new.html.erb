<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>

<%= form_for @user do |f| %>
<div>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>
</div>
<div>
  <%= f.label :email, "Email" %>
  <%= f.email_field :email %>
</div>
<div>
  <%= f.label :password,
    "Password: must be at least 5 characters long" %>
  <%= f.password_field :password %>
</div>
<div>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
</div>
<div>
  <%= f.submit %>
</div>
<% end %>

